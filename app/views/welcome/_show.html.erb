<section id="photos">

  <% @local_resturants[0..9].each do |restaurant| %>
    <div class="card" style="width: 18rem;">
      <img class="card-img-top" src="<%= @meals.sample["urls"]["small"] %>'" alt="Card image cap">
      <div class="card-body">
        <h5 class="card-title"> <%= restaurant[:name] %></h5>
        <a href="#" class="btn btn-primary"> Details</a>
      </div>
    </div>
  <% end %>
</section>



